{
  "name": "nodejs-restaurant-server",
  "version": "1.0.0",
  "description": "NodeJS Sample Restaurant Server",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "build": "sequelize-cli db:migrate && sequelize-cli db:seed:all",
    "test": "jest"
  },
  "contributors": [
    "Tiziano Pessa <tiziano@firenze.dev>",
    "Lorenzo Spinelli <lorenzo@firenze.dev>"
  ],
  "license": "MIT",
  "dependencies": {
    "@fastify/websocket": "^7.0.1",
    "apollo-datasource": "^3.3.2",
    "apollo-server-core": "^3.10.0",
    "apollo-server-fastify": "^3.10.0",
    "dataloader": "^2.1.0",
    "fastify": "^3.29.1",
    "graphql": "^16.5.0",
    "graphql-subscriptions": "^2.0.0",
    "graphql-ws": "^5.10.1",
    "lodash": "^4.17.21",
    "sequelize": "^6.21.3",
    "sqlite3": "^5.0.11"
  },
  "devDependencies": {
    "jest": "^29.0.2",
    "sequelize-cli": "^6.4.1"
  },
  "engines": {
    "node": "16.x"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/index.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 90,
        "lines": 90,
        "statements": 90
      }
    }
  }
}
